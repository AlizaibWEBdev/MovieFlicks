<style>
    .loader {
        width: 50px;
        aspect-ratio: 1;
        border-radius: 50%;
        border: 8px solid #0000;
        border-right-color: #ffa50097;
        position: relative;
        animation: l24 1s infinite linear;

    }

    .loader:before,
    .loader:after {
        content: "";
        position: absolute;
        inset: -8px;
        border-radius: 50%;
        border: inherit;
        animation: inherit;
        animation-duration: 2s;
    }

    .loader:after {
        animation-duration: 4s;
    }

    @keyframes l24 {
        100% {
            transform: rotate(1turn)
        }
    }

    body {
        font-family: system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
        margin: 0;
        padding: 0;
        overflow-x: hidden;

        text-decoration: none;

        box-sizing: border-box;
        font-size: 20px;
        background-color: #0b0f18;

    }


    h1 {
        text-align: center;
        font-size: 5vmax;
        color: white;

    }

    input {
        width: 50vw;
        padding: 14px;
        outline: none;
        border: 1px solid #1d75b9;
        font-size: 20px;
        background-color: #e8f0fe;
        border-radius: 20px 0px 0px 20px;
        transform: translateX(10px);

    }



    form button {
        padding: 13px;
        border-radius: 0px 20px 20px 0px;
        background-color: #e8f0fe;
        color: white;
        border: none;
        width: fit-content;
        display: flex;
        align-items: center;
        justify-content: center;
        font-size: 20px;
        cursor: pointer;

        border-left: none;
        border: 1px solid #1d75b9;
        outline: none;
    }

    form {
        margin-bottom: 20px;
    }

    .movies {
        display: flex;
        flex-wrap: wrap;
        align-items: center;
        justify-content: center;
        flex-direction: row;
    }

    .movie-card {

        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
        padding: 10px;
        width: calc(100vw/4);
        min-width: 300px;
        margin: auto;
        padding-top: 20px;
        text-align: start;
        background: #040609;
        border: 1px solid #007bff59;
        margin: 5px;
        border-radius: 10px;


    }

    .movie-card h2,
    p {
        font-weight: 400;
        font-size: 18px !important;
        width: 200px;

    }

    .movie-card h2,
    p {
        transform: translateX(5px);
    }

    @media only screen and (max-width:500px) {
        .movie-card {
            width: 80vw !important;
        }
    }

    a {
        text-decoration: none;
        color: rgb(255, 255, 255);
    }

    .movie-card p {
        font-size: 13px !important;

    }

    .movie-card img {

        height: auto;

    }

    .movie-card h2 {
        font-size: 18px;
        margin: 10px 0;
    }

    .movie-card p {
        font-size: 14px;
        margin: 5px 0;
    }

    .movie-card a {
        display: inline-block;
        padding: 8px 12px;
        margin-top: 10px;
        background: #007BFF;
        color: white;
        text-decoration: none;
        border-radius: 4px;
    }

    .movie-card a:hover {
        background: #0056b3;
    }

    .center {
        display: flex;
        align-items: center;
        justify-content: center;
        gap: 10px;
        flex-wrap: wrap;
    }

    .onett {
        width: 100vw;
        height: 100vh;
        position: fixed;
        top: 0;
        display: none;
        z-index: 9999;
        background-color: #040609;
    }
    .header {
        padding: 10px;


        display: flex;
        align-items: center;
        color: white;
        font-size: 30px;
        font-weight: 500;
        width: 100vw;
        position: absolute;
        top: 0;

    }

    .header .logo {
        transform: translateX(30px) !important;

        padding: 10px;
        border-radius: 10px;

        cursor: pointer;
    }

    .header .logo span {
        color: transparent;
        background: linear-gradient(45deg, #2ba3ff, darkblue);
        background-clip: text;
        font-size: 40px;
        font-weight: 900;
    }

    .hero {
        width: 100vw;
        align-items: center;
